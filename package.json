{
  "name": "be",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-p -r mockserver serve",
    "start:dev": "run-p -r serve:dev",
    "start:prod": "run-p -r mockserver serve:prod",
    "serve": "ng serve --aot --locale nl --sourcemap --proxy-config proxy.config.json",
    "serve:dev": "ng serve --aot --locale nl --environment=dnndev --sourcemap --proxy-config proxy.config.localdev.json",
    "serve:prod": "ng serve --prod --environment=dev --proxy-config proxy.config.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:before-nant": "node before-nant.js",
    "nant": "run-nant.bat",
    "build:release:dnn": "ng build --target=production --environment=dnn --deploy-url /DesktopModules/AOCRaad.Erkenningen.VakkenBeheer/",
    "build:dnn": "npm-run-all build:release:dnn build:before-nant nant",
    "build:netlify": "cp src/index.netlify.html src/index.html && ng build --target=production --environment=prod --output-hashing none",
    "mockserver": "node ./mocks/mocks.js",
    "tempdisabledprecommit": "lint-staged",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "co": "npm run cypress:open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.1.2",
    "@angular/common": "5.1.2",
    "@angular/compiler": "5.1.2",
    "@angular/core": "5.1.2",
    "@angular/forms": "5.1.2",
    "@angular/http": "5.1.2",
    "@angular/platform-browser": "5.1.2",
    "@angular/platform-browser-dynamic": "5.1.2",
    "@angular/router": "5.1.2",
    "@ngrx/effects": "5.2.0",
    "@ngrx/entity": "5.2.0",
    "@ngrx/router-store": "5.2.0",
    "@ngrx/store": "5.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.3",
    "date-fns": "1.29.0",
    "font-awesome": "4.7.0",
    "ngx-textarea-autosize": "1.1.1",
    "primeng": "5.2.4",
    "rxjs": "5.5.6",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.28",
    "@angular/cli": "8.3.6",
    "@angular/compiler-cli": "5.1.2",
    "@angular/language-service": "5.1.2",
    "@ngrx/store-devtools": "5.1.0",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "angular2-template-loader": "0.6.2",
    "codelyzer": "3.2.0",
    "husky": "0.14.3",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "6.0.0",
    "ng-apimock": "1.4.2",
    "ngrx-store-freeze": "0.2.1",
    "npm-run-all": "4.1.2",
    "prettier": "1.9.1",
    "ts-node": "4.0.2",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "tslint-config-prettier": "1.6.0",
    "typescript": "2.5.3",
    "wallaby-webpack": "3.9.3"
  },
  "lint-staged": {
    "*.{css,js,json,md,scss,ts}": [
      "prettier --write",
      "ng lint --type-check --fix",
      "git add"
    ]
  }
}
